    float AA = perm_Stage1_c1_c0(A) + P.z;
    float AB = perm_Stage1_c1_c0(A + 1.0) + P.z;
    float B = perm_Stage1_c1_c0(P.x + 1.0) + P.y;
    float BA = perm_Stage1_c1_c0(B) + P.z;
