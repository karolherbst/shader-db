    float g = noiseOctaves_Stage1_c1_c0(vec3(coords, uz_Stage1_c1_c0));
    float b = noiseOctaves_Stage1_c1_c0(vec3(coords, uz_Stage1_c1_c0));
    float a = noiseOctaves_Stage1_c1_c0(vec3(coords, uz_Stage1_c1_c0));
